#include<stdio.h>  
  
int main(){  
    float a,max = 0, min;  
    for(int i=0;i<10;i++){  
        scanf("%f",&a);  
          
        if(a>max)   max = a;  
        if(i==0){  
            min = a;  
            continue;  
        }      
        if(a<min)   min = a;  
    }  
    printf("maximum:%.2f\nminimum:%.2f\n",max,min);  
}  
